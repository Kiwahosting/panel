{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/.cache": true,
        "**/.vscode": true,
        "**/.npmrc": true,
        "**/.eslintrc": true,
        "**/.babelrc": true,
        ".gitmodules": true,
        "gatsby-ssr.js": true,
        "src/getPageContext.js": true,
        "src/html.js": true,
        "public": true,
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.cache/**": true,
        "**/public/**": true,
    },
    "editor.tabSize": 2
}